# initialise variables
var: result, [0]
var: num, [2]

# entry point
jmp: main
lbl: main_end
exit: 0

# main function
lbl: main

# start loop
lbl: start

# add 1 to num
add: $num, 1
# assign result to $num
mov: $num, $result

# print num
# print_num: 1, $num
# write: 1, "\n", 1

# while num != 100000
ne: $num, 100000
jc: $result, start

# end main
jmp: main_end
